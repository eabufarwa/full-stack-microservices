{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "proto/**/*.proto",
        "outDir": "dist"
      },
      {
        "include": "../contracts/proto/*.proto",
        "outDir": "dist/src/proto"
      }
    ]
  }
}
